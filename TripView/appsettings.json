{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "TripView": "Debug"
    }
  },
  "Startup": {
    "InitialPositionLatitude": 35.96422,
    "InitialPositionLongitude": -86.49259,
    "InitialZoomLevel": 9,
    "ZoomTimeInSeconds": 2,
    //Note: This is untested, but should allow you to use your own OSM tile server in place of using OSM's public instance.
    //"OpenStreetMapUrl": "https://tile.openstreetmap.org/{z}/{x}/{y}.png"
    //max time between trips (used to delineate data points)
    "MinutesBetweenTrips": 5,
    "UseSqlAsCache": true
  },
  "LeafspyImport": {
    //These must match your leafspy configuration to correctly import your data
    "DistanceUnit": "FEET", //Possible Values: FEET, METERS
    "CsvDelimiter": ","
  },
  "Colors": {
    "MapRouteColor": "#ff800080",
    "GpsAccuracyColor": "#400078d7",
    "ChartCrosshairColor": "#33333380",
    "ChartLineThickness": 1,
    "ChartBackgroundColor": "#ffffffff",
    "ChartPrimaryColor": "#ffff78d7",
    "ChartSecondaryColor": "#ff00b294",
    "ChartTertiaryColor": "#ffffb900",
    "ChartQuaternaryColor": "#ffe81123",
    "ChartQuinaryColor": "#ff5c2d91",
    "ChartSenaryColor": "#ff107c10",
    "ChartSeptenaryColor": "#ff107c10",
    "ChartOctonaryColor": "#ff2d7d9a",
    "ChartNonaryColor": "#ff7fba00",
    "ChartDenaryColor": "#ffb146c2"
  },
  "Chart": {
    "AirPressureUnit": "PSI", //Possible Values: PSI, BAR, KPA
    "DistanceUnit": "FEET", //Possible Values: FEET, METERS
    "TemperatureUnit": "FAHRENHEIT", //Possible Values: FAHRENHEIT, CELSIUS
    "TimeAxisLabelRotation": 60
  }
}